{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/timestamp",
        "function": "app"
      }
    ],
    "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
    ]
  }
}
